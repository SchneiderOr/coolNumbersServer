{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/randomNumbers",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/randomNumbers/random",
      "dest": "/index.js",
      "methods": [
        "GET"
      ]
    }
  ]
}